namespace HiMTA_Lect3;

using HiMTA_Lect3::ExtraInfo.Address;
using HiMTA_Lect3::ExtraInfo.Cars;


entity User {
    @Comment: 'Id of User'
    key usid : String(4);
    @Comment: 'Description'
    name : String(100);

    toCars : Association[*] to Cars on toCars.usid = usid;
    toAddress : Association[1] to Address on toAddress.usid = usid;
};